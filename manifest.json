{
	"name" : "GitHub News Filter",
	"version" : "0.3.0",
	"description" : "The GitHub News Filter allows you to filter the news feed on your github dashboard by category.",
	"icons" : { "16" : "icon16.png",
				"48" : "icon48.png",
				"128" : "icon128.png"},
	"content_scripts" : [
		{
			"matches" : ["https://github.com/","https://github.com/dashboard/*"],
			"css" : ["filter.css"],
			"js" : ["filter.min.js"],
			"run_at" : "document_end"
		}
	]
}